<!--pages/card_detail/card_detail.wxml-->
<import src="card_detail.skeleton.wxml" />
<view style="height: {{windowHeight-top_status_bar_height}}px;width: 100vw;position: fixed;z-index: 1000000;background-color: white;top:  {{top_status_bar_height}}px;" wx-if="{{loading}}">
  <template is="skeleton" wx-if="{{loading}}" />
</view>


<view style="position: fixed;top: {{menuButtonToTop}}px;width: 100vw; z-index: 98;left: 25rpx;">
  <image style="height:{{menuButtonHeight}}px;width: {{menuButtonHeight}}px;" src="/pages/images/back3.png" bindtap="backLastPage"></image>
</view>
<page-meta>
  <navigation-bar title="{{change_show?'卡片':'碎片'}}详情" background-color="{{top_bar_color}}" front-color="{{top_bar_font_color}}" color-animation-duratio="1500" color-animation-timing-func="linear" />
</page-meta>
<view style="position: fixed;width: 750rpx;height: {{top_status_bar_height}}px;z-index: 99;background-color: black;"></view>




<scroll-view show-scrollbar="false" enhanced="true" scroll-x wx-if="{{secodn_show}}">
  <swiper style="height:{{imgheight}}px;margin-top:  {{top_status_bar_height}}px;" bindchange="swiperChange" wx:if="{{data.images[0].type=='img'}}" current="{{current}}" bindlongpress="pickCardLongTap" bindtouchend="pickTouchEnd" catchtap="showBigImg" long>
    <swiper-item style="display: flex;justify-content: center;align-content:center;align-items: center; " wx:for="{{data.images}}">
      <image src="{{data.card_type=='SQUARE'&&change_show?'https://weiyu-bulletin-picture-1258021264.cos.ap-beijing.myqcloud.com/bulletin16365514400360_7265536201930709319.jpg':item.image_url}}" style="width: 100%;" mode='widthFix' class='img' />
      <!-- <view style="height: 30px; background-color: white;width: 100%;position: fixed;bottom: 0;"></view> -->

      <view wx:if="{{data.card_type=='SQUARE'&&!change_show}}" style="z-index: 99;">
        <image style="width: {{0.205*imgheight}}px;height: {{0.205*imgheight}}px;position: absolute;top:{{0.199*imgheight}}px;left: {{(window_width_px-0.199*imgheight)/2}}px;" src="/pages/images/square_blue.png"></image>

        <image style="width: {{0.169*imgheight}}px;height: {{0.169*imgheight}}px;position: absolute;top:{{0.539*imgheight}}px;left: {{(window_width_px-0.169*imgheight)/2}}px;" src="/pages/images/square_btn.png"></image>

        <!-- <image style="width: {{0.042*square_info_height}}rpx;height: {{0.042*square_info_height}}rpx;position: absolute;top:{{0.144*square_info_height}}rpx;left: {{(750-(0.538*square_info_height-3))/2+((0.538-0.149)*square_info_height)/2}}rpx;" src="{{item.account_icon}}"></image> -->

        <view style="position: absolute;top:{{0.199*imgheight+0.094*(0.205*imgheight)}}px;left: {{(window_width_px-0.199*imgheight)/2+0.094*(0.205*imgheight)}}px;">
          <view style="width: {{(0.205*imgheight)*0.812}}px;height: {{(0.205*imgheight)*0.812}}px;display: flex;justify-content: center;align-content: center;align-items: center;flex-direction: column;">
            <view style="display: flex; flex-direction: column;height: {{(0.205*imgheight)*0.812*0.99}}px;width: {{(0.205*imgheight)*0.812*0.99}}px;justify-content: space-between;align-items: center;align-content: center;">
              <!-- <view style="display: flex;flex-direction: row;margin-top: 3rpx; align-content: center;align-items: center;">
                <image style="width:{{(0.205*imgheight)*0.812*0.282}}px;height:{{(0.205*imgheight)*0.812*0.282}}px;border-radius: {{(0.205*imgheight)*0.812*0.282/2}}px;border:1px white solid;" src="{{data.account_icon}}"></image>
                <view style="display: flex;flex-direction: column;margin-left: 5rpx;font-size: {{(0.205*imgheight)*0.812*0.105}}px;color: white;font-weight: bold;">
                  <view style="width: {{(0.205*imgheight)*0.812*(0.99-0.282)-5}}px;word-break: break-all;text-overflow: ellipsis;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;">{{data.account_name}}</view>
                  <view style="width: {{(0.205*imgheight)*0.812*(0.99-0.282)-5}}px;">{{data.card_show_id}}</view>
                </view>
              </view> -->
              <image style="width:{{(0.079*imgheight)}}px;height:{{(0.079*imgheight)}}px;border-radius: {{(0.079*imgheight)/2}}px;border:1px white solid;" src="{{data.account_icon}}"></image>
              <view style="width: {{(0.205*imgheight)*0.812-5}}px;word-break: break-all;text-overflow: ellipsis;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;text-align: center;color: white;margin-top:  {{(0.01*imgheight)}}px;font-size: {{(0.027*imgheight)}}px;">{{data.account_name}}</view>
              <view style="display: flex;justify-content: center;align-content: center;align-items: center;width: 100%;margin-bottom: 3rpx;margin-top:  {{(0.01*imgheight)}}px;">
                <view style="font-size: {{(0.205*imgheight)*0.812*0.105}}px;color: white;font-weight: bold;width: 100%;text-align:center">购于 {{filter.format(data.createtime,'YY-MM-DD')}}</view>
              </view>
              <!-- <view style="display: flex;justify-content: center;align-content: center;align-items: center;width: 100%;margin-bottom: 3rpx;">
                <view style="font-size: {{(0.205*imgheight)*0.812*0.105}}px;color: white;font-weight: bold;width: 100%;text-align:center">购于 {{filter.format(data.createtime,'YY-MM-DD')}}</view>
              </view> -->
            </view>
          </view>
        </view>
        <!-- top  {{(0.01)*square_info_height+card_to_top+2}}rpx -->
        <!-- left   {{(750-(0.538*square_info_height-3))/2}}rpx -->
        <view style="position: absolute;font-size: {{(0.037*imgheight)}}px;color: #E4C383;left: {{0.348*imgheight}}px;;top: {{0.832*imgheight}}px;">{{data.card_show_id}}</view>

        <view style="color: white;position: absolute;font-size: {{0.031*imgheight}}px;top: {{0.309*imgheight}}px;left: {{0.038*imgheight}}px;">{{data.city_name}}</view>

        <view style="color: white;position: absolute;font-size: {{0.031*imgheight}}px;top: {{0.309*imgheight}}px;right: {{0.038*imgheight}}px;" wx:if="{{data.deal_num==0||data.deal_num==1}}">初次购买</view>
        <view style="color: white;position: absolute;font-size: {{0.031*imgheight}}px;top: {{0.309*imgheight}}px;right: {{0.038*imgheight}}px;" wx:else>交易{{data.deal_num}}次</view>

        <view style="color: white;position: absolute;font-size: {{0.037*imgheight}}px;top:{{0.415*imgheight}}px;left: {{0.098*imgheight}}px;">{{data.area_name}}</view>
        <view style="color: white;position: absolute;font-size: {{0.037*imgheight}}px;top: {{0.415*imgheight}}px;right: {{0.098*imgheight}}px;">第{{data.square_num}}块</view>

      </view>



    </swiper-item>
  </swiper>



  <!-- wx:elif="{{data.images[0].type=='video'}}" -->
  <view style="height:{{imgheight}}px;width: 750rpx;margin-top:  {{top_status_bar_height}}px;" wx:else bindtap="playVideo" bindlongpress="pickCardLongTap" bindtouchend="pickTouchEnd">
    <video style="height:{{imgheight}}px;width: 750rpx;" autoplay="{{true}}" controls="{{false}}" loop="{{true}}" muted="{{video_muted}}" src="{{data.images[0].video_url}}" object-fit="cover">
    </video>
  </view>

  <view style="position: absolute;margin-top:  {{-(imgheight)/2}}px;left: {{(window_width_px-48*2.5)/2}}px;height:{{45*2.5}}px;width: {{48*2.5}}px;opacity: 0;z-index: 1000;" bindlongpress="pickCardLongTap" bindtouchend="pickTouchEnd" animation="{{pick_toast_ant}}" wx:if="{{!change_show}}">
    <image style="height:{{45*2.5}}px;width: {{48*2.5}}px;" src="/pages/images/pick_toast.png">
    </image>
  </view>

  <view style="width: 50rpx;height: 50rpx; position: absolute; top: {{imgheight-22}}px;left: 23rpx;z-index: 50;border-radius: 25rpx;background-color: #212121;" wx:if="{{data.images[0].type=='video'}}" bindtap="changeMute">
    <view style="width: 50rpx;height: 50rpx;display: flex;justify-content: center;align-items: center;align-content: center;">
      <image style="width: 30rpx;height: 30rpx;" src="{{video_muted_icon}}">
      </image>
    </view>
  </view>


  <view style="width: 197rpx;height: 72rpx; position: absolute; top: {{imgheight-27}}px;right: 0rpx;z-index: 50;" bindtap="getTrueCardInfo" wx:if="{{data.card_type!='SQUARE'}}">
    <view style="width: 197rpx;height: 72rpx;display: flex;justify-content: center;align-items: center;align-content: center;">
      <image style="width: 197rpx;height: 72rpx;" src="/pages/images/get_ture_card1.png">
      </image>
    </view>
  </view>




  <image style="width: 180rpx;height: 180rpx; position: absolute; top: 138rpx;left: 18rpx;z-index: 50;opacity: {{badge_opacity}};" src="{{data.card_badge_url}}/limit_width_180" wx:if="{{data.have_card_badge}}" bindtap="showBadge">
  </image>

  <!-- <image style="width: 100rpx;height: 100rpx; position: absolute; top: 138rpx;right: 18rpx;z-index: 50;" src="/pages/images/hot_white.png" bindtap="showAllInfo" wx:if="{{data.hot_card}}">
  </image> -->

  <view style="display: flex;flex-direction: row;  margin-top: 10rpx;" wx:if="{{data.images.length>1}}">
    <view class="dots">
      <block wx:for="{{data.images}}" wx:for-index="index" wx:key="*this">
        <view class="{{current==index?'active':''}}"></view>
      </block>
    </view>
  </view>



  <view class="acount_info" style="margin-top: {{data.images.length>1?'10':'25'}}rpx;">
    <View style="width: 32%;height: 115rpx;border-radius: 13rpx;background-color: #858BC7;" bindtap="showCardNum">

      <view style="position: relative;top: -10rpx;left: {{data.have_card_num>99?'-15':'-10'}}rpx;height: 40rpx;width: {{data.have_card_num>99?'45':'40'}}%;background-color: #F46563;border-radius: 10rpx;color: white;font-size: 20rpx;font-weight: bold;">
        <view style="display: flex;width: 100%;height: 100%;justify-content: center;align-items: center;align-content: center;">
          <view style="display: flex;">拥有:{{data.have_card_num}}块</view>
        </view>
      </view>



      <view class="top_info_item1">
        <text style="color: white;font-size: 25rpx;font-weight: bold;">{{change_show?'卡片':'碎片'}}</text>
        <view class="top_info_item_item">
          <image style="width: 70rpx;height: 70rpx;" src="/pages/images/ssr.png" wx:if="{{data.card_type=='SSR'}}">
          </image>
          <image style="width: 70rpx;height: 70rpx;" src="/pages/images/ssr.png" wx:if="{{data.card_type=='BONUS'}}">
          </image>
          <image style="width: 70rpx;height: 70rpx;" src="/pages/images/sr.png" wx:if="{{data.card_type=='SR'}}">
          </image>
          <image style="width: 70rpx;height: 70rpx;margin-left: -20rpx;" src="/pages/images/square.png" wx:if="{{data.card_type=='SQUARE'}}">
          </image>
          <image style="width: 70rpx;height: 70rpx;" src="/pages/images/true_card.png" wx:if="{{data.card_type=='TRUE_CARD'}}">
          </image>
          <image style="height: 70rpx;width: 70rpx;" src="/pages/images/function.png" wx:if="{{data.card_type=='FUNCATION'}}"></image>
          <image style="height: 70rpx;width: 70rpx;" src="/pages/images/infiniteness.png" wx:if="{{data.card_type=='FUNCATION'}}"></image>
          <image style="height: 70rpx;width: 70rpx;" src="/pages/images/infiniteness.png" wx:elif="{{data.card_type=='BONUS'}}"></image>

          <text style="color: white;font-size: 25rpx;font-weight: bold;" wx:elif="{{data.card_type!='FUNCATION'}}">{{data.surplus_card_num}}/{{data.total_card_num}}</text>
        </view>
      </view>
    </View>
    <View style="width: 32%;height: 115rpx;border-radius: 13rpx;background-color: #58B8B7;" bindtap="showCardLv">
      <view class="top_info_item" wx:if="{{data.card_type=='SQUARE'}}">
        <text style="color: white;font-size: 25rpx;font-weight: bold;">等级</text>
        <view class="top_info_item_item">
          <image style="width: 70rpx;height: 70rpx;" src="/pages/images/c_info_lv.png"></image>
          <image style="height: 70rpx;width: 70rpx;" src="/pages/images/infiniteness.png" wx:if="{{data.card_type=='FUNCATION'}}"></image>
          <text style="color: white;font-size: 25rpx;font-weight: bold;margin-left: 9rpx;" wx:if="{{data.card_type!='FUNCATION'}}">
            <block wx:if="{{data.card_type!='SQUARE'}}">{{data.pick_num}}</block>
            <block wx:else>{{data.card_lv}}</block>
          </text>
        </view>
      </view>
      <view class="top_info_item" wx:elif="{{data.card_type!='SQUARE'}}">
        <text style="color: white;font-size: 25rpx;font-weight: bold;">Pick</text>
        <view class="top_info_item_item" style="margin-top: 5rpx;">
          <image style="width: 110rpx;height:55rpx;" src="/pages/images/pick.png"></image>
          <image style="height: 70rpx;width: 70rpx;" src="/pages/images/infiniteness.png" wx:if="{{data.card_type=='FUNCATION'}}"></image>
          <image style="height: 70rpx;width: 70rpx;margin-top: -10rpx;" src="/pages/images/infiniteness.png" wx:elif="{{data.card_type=='BONUS'}}"></image>

          <text style="color: white;font-size: 25rpx;font-weight: bold;margin-left: 9rpx;" wx:elif="{{data.card_type!='FUNCATION'}}">
            <block wx:if="{{data.card_type!='SQUARE'}}">{{data.pick_num}}</block>
            <block wx:else>{{data.card_lv}}</block>
          </text>
        </view>
      </view>
    </View>
    <View style="width: 32%;height: 115rpx;border-radius: 13rpx;background-color: #E4C383;" bindtap="showCardCPoint">
      <view class="top_info_item">
        <text style="color: white;font-size: 25rpx;font-weight: bold;">C-point</text>
        <view class="top_info_item_item2">
          <image style="width: 55rpx;height: 55rpx;" src="/pages/images/assess_white.png" wx:if="{{data.assessing}}"></image>
          <image style="width: 55rpx;height: 55rpx;" src="/pages/images/c_info_point.png" wx:elif="{{data.card_type!='FUNCATION'}}"></image>

          <text style="color: white;font-size: 25rpx;font-weight: bold;margin-left: 9rpx;" wx:if="{{data.assessing}}">宇宙首发</text>
          <image style="height: 70rpx;width: 70rpx;" src="/pages/images/infiniteness.png" wx:elif="{{data.card_type=='FUNCATION'||data.card_type=='SQUARE'}}"></image>
          <text style="color: white;font-size: 25rpx;font-weight: bold;margin-left: 9rpx;" wx:elif="{{data.card_type!='FUNCATION'}}">{{data.card_cpoint}}</text>

          <!-- <view wx:if="{{!item.assessing}}">{{card_id==item.card_id?cp:item.card_cpoint}}</view>
            <view wx:if="{{item.assessing}}">宇宙首发</view> -->
        </view>
      </view>
    </View>

  </view>
  <view class="content" wx:if="{{(data.title.length>0||data.content.length>0)}}">
    <view style="font-size: 35rpx;margin-left: 4%;margin-right: 4%;word-break: break-all;font-weight: bold;" wx:if="{{data.title.length>0}}">{{data.title}}</view>
    <view style="font-size: 31rpx;margin-left: 4%;margin-top: {{data.title.length>0?'30':'0'}}rpx;;margin-right: 4%;word-break: break-all; " wx:if="{{data.content.length>0}}"><text>{{data.content}}</text></view>
  </view>
  <l-badge value="{{data.card_show_id}}" show="{{true}}" shape="midid" style="display: flex;margin-top: 25rpx;" bindtap="showIdToast">
    <!-- 插槽内的内容位于徽标左下角 -->
    <view style="width: 100vw;justify-content: center;align-items: center;align-content: center;display: flex;">
      <image style="width: 571rpx;height: 69rpx;" src="/pages/images/id_bar.png">
      </image>
    </view>
  </l-badge>
  <view style="display:flex;flex-direction: row;justify-content: center;align-content: center;align-items: center;margin-top: 15rpx;" bindtap="toCardCodePage">
    <view style="font-size: 24rpx;line-height: 24rpx;color: #E6C284;">您拥有{{data.have_card_num}}个{{change_show?'卡片':'碎片'}}码</view>
    <image style="width: 34rpx;height: 34rpx;" src="/pages/images/card_code_next.png"></image>
  </view>



  <view class="other_info" wx:if="{{data.card_type=='FUNCATION'}}">
    <view style="display: flex;justify-content: center;align-items: center; ">
      <image style="width: 60rpx;height: 60rpx; " src="/pages/images/function_card.png" data-item="{{data.account_id}}"></image>
      <text style="margin-left: 15rpx;font-size: 27rpx;max-width: 250rpx;white-space:nowrap;text-overflow:ellipsis;-webkit-text-overflow:ellipsis;overflow:hidden;color: #27A2DF;">{{data.title}}</text>
    </view>
    <view style="display: flex;justify-content: center;align-items: center; ">
      <image style="width: 27rpx; height: 27rpx; margin-top: 2rpx;" src="/pages/images/location1.png'" wx:if="{{data.place_name.length>0}}"></image>
      <text style=" font-size: 27rpx;color: #27A2DF;margin-left: 4rpx;margin-right: 15rpx;max-width: 250rpx;white-space:nowrap;text-overflow:ellipsis;-webkit-text-overflow:ellipsis;overflow:hidden;">{{data.place_name}}</text>
      <text style="font-size: 27rpx; color: #27A2DF;">{{filter.format(data.createtime,'MM-DD')}}</text>
    </view>
  </view>

  <view class="other_info" wx:elif="{{data.card_type=='TRUE_CARD'}}">
    <view style="display: flex;justify-content: center;align-items: center; ">
      <image style="width: 60rpx;height: 60rpx; " src="/pages/images/true_card_no_bg.png" data-item="{{data.account_id}}"></image>
      <text style="margin-left: 15rpx;font-size: 27rpx;max-width: 250rpx;white-space:nowrap;text-overflow:ellipsis;-webkit-text-overflow:ellipsis;overflow:hidden;color: #D545C1;">实物{{change_show?'卡片':'碎片'}}</text>
    </view>
    <view style="display: flex;justify-content: center;align-items: center; ">
      <image style="width: 27rpx; height: 27rpx; margin-top: 2rpx;" src="/pages/images/location1.png'" wx:if="{{data.place_name.length>0}}"></image>
      <text style=" font-size: 27rpx;color: #D545C1;margin-left: 4rpx;margin-right: 15rpx;max-width: 250rpx;white-space:nowrap;text-overflow:ellipsis;-webkit-text-overflow:ellipsis;overflow:hidden;">{{data.place_name}}</text>
      <text style="font-size: 27rpx; color: #D545C1;">{{filter.format(data.createtime,'MM-DD')}}</text>
    </view>
  </view>

  <view class="other_info" wx:else>
    <view style="display: flex;justify-content: center;align-items: center; ">
      <image style="width: 60rpx;height: 60rpx; border-radius: {{data.card_type=='SSR'?'0':'40'}}rpx;" src="{{data.card_type=='SSR'?'/pages/images/ssr_no_bg.png':data.account_icon}}" bindtap="{{data.card_type=='SSR'?'':'toAccountInfo'}}" data-item="{{data.account_id}}"></image>
      <text style="margin-left: 15rpx;font-size: 27rpx;max-width: 250rpx;white-space:nowrap;text-overflow:ellipsis;-webkit-text-overflow:ellipsis;overflow:hidden;color: {{data.card_type=='SSR'?'#E4C383':'#000000'}};">{{change_show?'卡片':(data.card_type=='SSR'?'稀有碎片':data.account_name)}}</text>
    </view>
    <view style="display: flex;justify-content: center;align-items: center; ">
      <image style="width: 27rpx; height: 27rpx; margin-top: 2rpx;" src="{{data.card_type=='SSR'?'/pages/images/location3_gold.png':'/pages/images/location1.png'}}" wx:if="{{data.place_name.length>0}}" bindtap="toMap"></image>
      <text style=" font-size: 27rpx;color: {{data.card_type=='SSR'?'#E4C383':'#33A3DE'}};margin-left: 4rpx;margin-right: 15rpx;max-width: 250rpx;white-space:nowrap;text-overflow:ellipsis;-webkit-text-overflow:ellipsis;overflow:hidden;" bindtap="toMap">{{data.place_name}}</text>
      <text style="font-size: 27rpx; color: {{data.card_type=='SSR'?'#E4C383':'#8D8D8D'}};">{{filter.format(data.createtime,'MM-DD')}}</text>
    </view>
  </view>

  <view style="margin-left:4%;margin-right: 4%; background-color: rgba(128, 128, 128, 0.226); height: 1rpx;margin-top: 25rpx;" wx:if="{{data.collection_badge_id>0}}">
  </view>

  <view style="display: flex;flex-direction: row; margin-top: 25rpx;" wx:if="{{data.collection_badge_id>0}}">
    <view style="display: flex;padding-top: 10rpx;padding-bottom: 10rpx;padding-left: 15rpx;padding-right: 25rpx;background-color: #D545C1;border-radius: 0 10rpx 10rpx 0;font-size: 20rpx;color: white;width: auto;">
      <view>集齐{{badgeInfo.have_num}}/{{badgeInfo.ssr_num+badgeInfo.sr_num}}块稀有{{change_show?'卡片':'碎片'}}，点亮[{{badgeInfo.badge_name}}]徽章，即可兑换</view>
    </view>
  </view>

  <view style="margin-left: 3%;width: 94%;" wx:if="{{data.collection_badge_id>0}}">
    <view style=" margin-left: 2%;margin-right: 2%;padding-top: 2%; ">
      <view style="display: flex;flex-direction: row; ">
        <view style="display: flex;flex-direction: column;margin-left: 10rpx;margin-top: 15rpx;max-width: 290rpx;align-items: center;justify-content: center;">
          <view>
            <view bindtap="toCardInfoPage">
              <view style="height: {{140+85}}rpx;">
                <view style="display: flex;flex-direction: row; align-items: center;justify-content: center;align-content: center;width: 290rpx; margin-top: 0;" wx:if="{{imgUrls[current2].title.length>0}}">
                  <view style="font-size: 31rpx;font-weight: bold;width: 290rpx;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp:3;overflow: hidden;word-break: break-all;" wx:if="{{imgUrls[current2].content.length>0}}">{{imgUrls[current2].title}}</view>
                  <view style="font-size: 31rpx;font-weight: bold;width: 290rpx;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 4;overflow: hidden;word-break: break-all;" wx:else>{{imgUrls[current2].title}}</view>
                </view>
                <!-- 无标题时显示内容 -->
                <view style="display: flex;flex-direction: row; align-items: center;justify-content: center;align-content: center;width: 290rpx;margin-top: 0;" wx:elif="{{imgUrls[current2].title.length<=0&&imgUrls[current2].content.length>0}}">
                  <view style="font-size: 31rpx;font-weight: bold;width: 290rpx;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp:4;overflow: hidden;word-break: break-all;">{{imgUrls[current2].content}}</view>
