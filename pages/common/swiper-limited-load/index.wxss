/* components/swiper-limited-load/index.wxss */

.item_class {}


@keyframes showCp {
  from {
    opacity:0
  }

  to {
    opacity:1
  }
}

@keyframes hideCp {
  from {
    opacity:1
  }
  to {
    opacity:0
  }
}

.show-cp-ant {
  animation: showCp 0.2s linear 1;
}

.hide-cp-ant {
  animation: hideCp 0.4s linear 1;
}