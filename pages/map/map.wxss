}

.control-btn {
  position: absolute;
  height: 85rpx;
  width: 85rpx;
  z-index: 95;
  /* background: #FFF; */
  line-height: 35px;
  font-size: 30px;
  text-align: center;
  border-radius: 4px;
}

.control-btn-bar {
  position: absolute;
  height: 50rpx;
  z-index: 94;
  /* background: #FFF; */
  line-height: 50rpx;
  font-size: 30rpx;
  text-align: center;
  border-radius: 0 15rpx 15rpx 0;
  padding-left: 25rpx;
  padding-right: 15rpx;

}

.center-sign {
  position: absolute;
  height: 120px;
  width: 120px;
  z-index: 95;
  line-height: 35px;
  font-size: 30px;
  text-align: center;
  margin: auto;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.mask {
  position: absolute;
  height: 100vh;
  width: 100vw;
  z-index: 99;
  background-color: #000000CC;
  margin: auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}

.card_bottom_bar {
  position: absolute;
  height: 5vh;
  width: 30vw;
  z-index: 120;
  border-radius: 2vh;
  background-color: black;
  color: white;
  margin: auto;
  left: 0;
  bottom: 15rpx;
  right: 0;
}

.get_card_loading {
  position: absolute;
  height: 1rpx;
  width: 1rpx;
  z-index: 100;
  left: 50%;
  top: 30%;
}

.get_card1 {
  position: absolute;
  height: 16rpx;
  width: 9rpx;
  z-index: 112;
  left: 50%;
  top: 30%;
  transform: translate(-50%, -50%);
}

.get_card2 {
  position: absolute;
  height: 16rpx;
  width: 9rpx;
  z-index: 111;
  left: 50%;
  top: 30%;
  transform: translate(-50%, -50%);
}

.get_card3 {
  position: absolute;
  height: 16rpx;
